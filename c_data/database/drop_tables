 SELECT 'ALTER TABLE '||OWNER||'.'||TABLE_NAME||' DROP CONSTRAINT '||CONSTRAINT_NAME||';'  FROM DBA_CONSTRAINTS WHERE OWNER='REPORTDBA'
 AND TABLE_NAME NOT IN('TABLESPACES_DB','SEGMENTS_DB','WEB_USERS')
 ;

 SELECT 'DROP SEQUENCE '||SEQUENCE_OWNER||'.'||SEQUENCE_NAME||';' FROM DBA_SEQUENCES WHERE SEQUENCE_OWNER='REPORTDBA';

 SELECT 'DROP TABLE '||OWNER||'.'||TABLE_NAME||' PURGE;' FROM DBA_TABLES 
 WHERE OWNER='REPORTDBA'
 AND TABLE_NAME NOT IN('TABLESPACES_DB','SEGMENTS_DB','WEB_USERS')
 ;
 
 